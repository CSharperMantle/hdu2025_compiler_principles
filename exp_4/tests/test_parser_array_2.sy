int main() {
    int matrix[3][3] = {{1,2,3},{4,5,6},{7,8,9}};
    int i = 0;
    int j = 0;
    int count = 0;
    
    while (i < 3) {
        j = 0;
        while (j < 3) {
            if (matrix[i][j] > 4 && (i + j) % 2 == 0) {
                count = count + 1;
            } else if (matrix[i][j] == 5 || matrix[i][j] == 7) {
                count = count - 1;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    
    return count;
}