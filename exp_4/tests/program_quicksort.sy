const int N = 10;

int partition(int arr[], int low, int high) {
    int pivot = arr[high];
    int i = low - 1;
    int j = low;

    while (j < high) {
        if (arr[j] < pivot) {
            i = i + 1;
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }
        j = j + 1;
    }

    int temp = arr[i + 1];
    arr[i + 1] = arr[high];
    arr[high] = temp;

    return i + 1;
}

void quicksort(int arr[], int low, int high) {
    if (low < high) {
        int pi = partition(arr, low, high);
        quicksort(arr, low, pi - 1);
        quicksort(arr, pi + 1, high);
    }
}

int main() {
    int arr[N] = {64, 34, 25, 12, 22, 11, 90, 88, 45, 50};
    int i = 0;

    quicksort(arr, 0, N - 1);

    // Verify sorting by checking if array is in ascending order
    int sorted = 1;
    i = 0;
    while (i < N - 1) {
        if (arr[i] > arr[i + 1]) {
            sorted = 0;
        }
        i = i + 1;
    }

    return sorted;
}